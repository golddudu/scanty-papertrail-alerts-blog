<h1>Latest : </h1>
<table class="archive">
<% posts.each do |post| %>
	<tr>
	<!--	<td class="date"><%= post.created_at.strftime("%b %d %Y at %H:%M") %></td> -->
		 <td class="date"><%= post.created_at.strftime("-") %>
		<td class="title"><a href="<%= post.url %>"><%= post.title %></a></td>
	</tr>
<% end %>
</table>
